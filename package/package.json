{
  "name": "predictive-textarea",
  "version": "0.2.2",
  "description": "A rich text input with AI-powered autocomplete content predictions for React",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run compile:esm && npm run compile:cjs",
    "compile:esm": "tsc -p tsconfig.esm.json",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "watch": "tsup --config tsup.config.ts --watch",
    "dev": "cd ../dev && yarn dev",
    "lint": "eslint src/**/*.ts*",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare-version": "tsx scripts/release-branch.ts prepare-version",
    "create-release": "tsx scripts/release-branch.ts create-release"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.7",
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.22.0",
    "@stylistic/eslint-plugin": "4.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "8.30.1",
    "@typescript-eslint/parser": "8.26.1",
    "@vitejs/plugin-react": "^4.4.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.2",
    "esbuild-plugin-preserve-directives": "^0.0.11",
    "eslint": "9.24.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-perfectionist": "4.10.1",
    "eslint-plugin-yenz": "1.2.0",
    "execa": "^9.5.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "octokit": "^4.1.3",
    "ora": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0",
    "undici": "^7.8.0",
    "vite": "^5.4.18",
    "vite-plugin-dts": "^3.9.1"
  },
  "dependencies": {
    "openai": "^4.95.1",
    "server-only": "^0.0.1",
    "trie-search": "^2.2.0"
  },
  "packageManager": "yarn@4.9.1+sha512.f95ce356460e05be48d66401c1ae64ef84d163dd689964962c6888a9810865e39097a5e9de748876c2e0bf89b232d583c33982773e9903ae7a76257270986538"
}
